<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructure">

    <application
        android:allowBackup="true"
        android:icon="@drawable/international_music"
        android:label="@string/app_name"
        android:roundIcon="@drawable/international_music"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity
            android:name=".Pop"
            android:label="@string/pop" />

        <activity
            android:name=".Electronic"
            android:label="@string/electronic" />

        <activity
            android:name=".Rap"
            android:label="@string/rap" />

        <activity
            android:name=".Classical"
            android:label="@string/classic" />

    </application>

</manifest>